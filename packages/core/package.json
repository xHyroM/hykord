{
  "name": "hykord",
  "version": "0.0.1",
  "author": "xHyroM",
  "devDependencies": {
    "babel-minify": "^0.5.2",
    "electron": "13.6.9",
    "semver": "^7.3.7",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "_moduleAliases": {
    "@hykord": "renderer/api/modules",
    "@dependency": "dependencies",
    "@api": "renderer/api",
    "@ipc": "renderer/ipc"
  },
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "ttsc -p . && minify dist -d dist",
    "postbuild": "rm -rf dist/dependencies && cp package.json ./dist/package.json && cp -r ./src/dependencies/ ./dist/dependencies && rm ./dist/dependencies/**/node_modules/ -rf"
  }
}