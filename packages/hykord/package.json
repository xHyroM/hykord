{
  "name": "hykord",
  "version": "0.0.1",
  "author": "xHyroM",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc -p .",
    "postbuild": "rm -rf dist/dependencies && cp -r ./src/dependencies/ ./dist/dependencies && rm -rf ./dist/dependencies/**/node_modules/ && minimize-js ./dist/ -w -i -s -d"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "minimize-js": "^1.3.0",
    "module-alias": "^2.2.2",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "electron": "^20.1.0"
  },
  "_moduleAliases": {
    "@hykord": "dist/hykord",
    "@dependency": "dist/dependencies",
    "@module": "dist/hykord/api/modules"
  }
}