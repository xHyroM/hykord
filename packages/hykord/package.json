{
  "name": "hykord",
  "version": "0.0.1",
  "author": "xHyroM",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.2",
    "electron": "13.6.9",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "_moduleAliases": {
    "@hykord": "hykord/api/modules",
    "@dependency": "dependencies",
    "@api": "hykord/api"
  },
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "ttsc -p . && babel dist -d dist",
    "postbuild": "rm -rf dist/dependencies && cp package.json ./dist/package.json && cp -r ./src/dependencies/ ./dist/dependencies && rm ./dist/dependencies/**/node_modules/ -rf"
  }
}